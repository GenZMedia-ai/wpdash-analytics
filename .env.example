# Supabase Configuration
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY

# Security Secrets
INGEST_SECRET=YOUR_INGEST_SECRET
HMAC_SECRET=YOUR_HMAC_SECRET_AT_LEAST_32_CHARS

# Dashboard Authentication
DASHBOARD_PASSWORD_HASH=YOUR_BCRYPT_PASSWORD_HASH
JWT_SECRET=YOUR_JWT_SECRET_AT_LEAST_32_CHARS

# PHP Proxy Configuration
ALLOWED_ORIGINS=https://yourdomain.com,https://www.yourdomain.com

# Edge Function Environment (set in Supabase dashboard)
# INGEST_SECRET=same_as_above
# HMAC_SECRET=same_as_above